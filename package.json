{
  "name": "shadowsocks-electron",
  "version": "1.2.0",
  "description": "Shadowsocks GUI client with cross-platform desktop support",
  "author": {
    "name": "nojsja",
    "email": "yangwei13@outlook.com"
  },
  "repository": "https://github.com/nojsja/shadowsocks-electron",
  "license": "GPL-3.0",
  "homepage": ".",
  "main": "public/electron.js",
  "private": true,
  "scripts": {
    "start:renderer": "cross-env PORT=3001 cross-env BROWSER=none react-app-rewired start",
    "start:main": "nodemon --config nodemon.json",
    "start:main:inspect": "yarn build:main && cross-env INSPECT=1 electron . --inspect=5222",
    "start": "concurrently \"yarn start:renderer\" \"wait-on http://localhost:3001 && yarn start:main\"",
    "build:renderer": "react-app-rewired build",
    "build:main": "yarn tsc -p main/tsconfig.json",
    "build": "yarn build:main && yarn build:renderer",
    "pack:linux": "electron-builder --linux",
    "pack:linux:debug": "electron-builder --linux --config electron-builder-debug.json",
    "pack:mac": "electron-builder --mac",
    "pack:win": "electron-builder --win",
    "publish:linux": "electron-builder --linux --p always",
    "publish:mac": "electron-builder --mac --p always",
    "publish:win": "electron-builder --win --p always",
    "publish:linux:win": "electron-builder --win --linux --p always",
    "publish:snap": "snapcraft login && snapcraft upload dist/shadowsocks-electron_1.1.2_amd64.snap",
    "eject": "react-app-rewired eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "clean": "rm -rf dist/linux-*unpacked && rm -rf dist/__snap* && rm -rf dist/win-*unpacked",
    "lint": "eslint --ext .tsx,.ts .",
    "typecheck": "tsc --noUnusedLocals",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@sentry/electron": "^3.0.5",
    "@types/i18n": "^0.13.2",
    "chalk": "4.x",
    "clsx": "^1.1.1",
    "electron-is-dev": "1.1.0",
    "electron-re": "1.2.2",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.6.5",
    "electron-window-state": "^5.0.3",
    "fs-extra": "9.0.0",
    "i18n": "^0.14.2",
    "i18next": "^20.6.1",
    "ipv6": "*",
    "js-base64": "^2.4.0",
    "jsqr": "^1.4.0",
    "node-fetch": "2.6.0",
    "node-udp-forwarder": "^0.2.0",
    "open": "7.0.3",
    "qrcode": "^1.4.4",
    "rc-field-form": "^1.26.2",
    "react-i18next": "^11.12.0",
    "socks": "1.x",
    "tapable": "^2.2.1",
    "v8-compile-cache": "^2.3.0",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@types/i18next": "^13.0.0",
    "@types/js-base64": "^2.3.0",
    "@types/node": "12.12.26",
    "@types/node-fetch": "2.5.5",
    "@types/qrcode": "^1.4.1",
    "@types/react": "16.9.32",
    "@types/react-dom": "16.9.6",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.3",
    "@types/uuid": "7.0.2",
    "babel-plugin-import": "1.13.0",
    "compressing": "^1.5.1",
    "concurrently": "5.1.0",
    "cross-env": "^7.0.3",
    "customize-cra": "0.9.1",
    "dotenv": "8.2.0",
    "electron": "18.0.3",
    "electron-builder": "^22.14.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-notarize": "0.3.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "4.2.3",
    "immer": "^9.0.14",
    "lint-staged": "10.0.9",
    "nodemon": "2.0.2",
    "notistack": "^1.0.10",
    "patch-package": "^6.4.7",
    "prettier": "2.0.2",
    "react": "16.13.1",
    "react-app-rewire-react-svg-loader": "^1.0.1",
    "react-app-rewired": "2.1.5",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.1",
    "redux": "4.0.5",
    "redux-persist": "6.0.0",
    "redux-persist-electron-storage": "2.1.0",
    "redux-thunk": "^2.3.0",
    "source-map-explorer": "2.4.1",
    "targz": "1.0.1",
    "typeface-roboto": "0.0.75",
    "typescript": "3.8.3",
    "use-bus": "^2.5.0",
    "uuid": "7.0.2",
    "v8-inspect-profiler": "^0.1.0",
    "wait-on": "4.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn clean && lint-staged && yarn lint && yarn typecheck"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "Electron >= 8"
  ]
}
